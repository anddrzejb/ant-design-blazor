<div>
    <MultiRangeSlider AllowOverlapping="@_overlap">
        <RangeItem DefaultValue="(0, 40)"/>
        <RangeItem DefaultValue="(35, 60)"/>
        <RangeItem DefaultValue="(65, 100)"/>
    </MultiRangeSlider>
    Allow Overlapping: <Switch Size="small" @bind-Value="@_overlap" />
    <br/>
    Will continue showing overlapping when AllowOverlapping="true", but during movement it will try to adjust to adhere to no overlapping rule.
</div>

@code
{
    private bool _overlap = false;
}